{
  "name": "ezla",
  "version": "10.31.0",
  "private": true,
  "scripts": {
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx": "run-p dx:* --print-label",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.31.0",
    "@trpc/next": "^10.31.0",
    "@trpc/react-query": "^10.31.0",
    "@trpc/server": "^10.31.0",
    "clsx": "^1.1.1",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.8.8",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.1.2",
    "vitest": "^0.32.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
