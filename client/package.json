{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env VITE_BASE_API_URL=http://localhost:3000 vite",
    "build": "vite build --mode production",
    "build-staging": "vite build --mode staging",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "generate:api:client": "npx @openapitools/openapi-generator-cli generate -i ../openapi.yaml -g typescript-fetch -o src/api-client/ --skip-validate-spec",
    "preview": "vite preview",
    "postinstall": "npm run generate:api:client"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "@mui/x-date-pickers": "^6.8.0",
    "@tanstack/react-table": "^8.9.2",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.8",
    "firebase": "^9.6.10",
    "generate-password-browser": "^1.1.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-router-dom": "^6.13.0",
    "react-verification-input": "^3.3.0",
    "sort-by": "^1.2.0",
    "stylis": "^4.2.0",
    "stylis-plugin-rtl": "^2.1.1",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/stylis": "^4.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "firebase": "^9.6.10",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.0",
    "vite": "^4.5.1"
  }
}
